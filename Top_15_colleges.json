GET universities/canadian_universities/_search

{
  "_source": ["*"],
  "size": 15,
  "query": {
    "bool": {
      "must": [
        {
          "exists": {
            "field": "candianRanking"
          }
        },
        {
          "wildcard": {
            "_index": "*College*"
          }
        }
      ]
    }
  },
  "sort": [
    {"candianRanking": {"order": "asc"}}
  ]
}